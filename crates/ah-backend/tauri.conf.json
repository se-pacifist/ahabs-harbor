{
  "build": {
    "beforeDevCommand": "cd ah-frontend && trunk serve",
    "beforeBuildCommand": "cd ah-frontend && trunk build",
    "devPath": "http://localhost:1420",
    "distDir": "../ah-frontend/dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Ahabs Harbor",
    "version": "0.0.0"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "se.pacifist.ahabs-harbor",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Ahabs Harbor",
        "width": 800,
        "height": 600,
        "transparent": true,
        "decorations": true
      }
    ]
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
